<% if @request.request_type.name == "फौती" %>
  <%= render partial: "requests/fouti/fouti_show" %>
<% elsif @request.request_type.name == "नामांतरण" %>
  <%= render partial: "requests/namantaran/namantaran_show" %>
<% elsif @request.request_type.name == "बटवारा (पिता-पुत्र)" %>
  <%= render partial: "requests/batwara/pita_putra_batwara_show" %>
<% elsif @request.request_type.name == "बटवारा (आपसी सहमति)" %>
  <%= render partial: "requests/batwara/sah_khatedar_batwara_show" %>
<% end %>